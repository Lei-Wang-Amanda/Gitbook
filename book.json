{
    "plugins": [
        "back-to-top-button",
        "chapter-fold",
        "code",
        "splitter",
        "-lunr",
        "-search",
	"-sharing",
        "search-pro",
        "insert-logo",
        "custom-favicon",
        "pageview-count",
        "tbfed-pagefooter",
        "popup",
	"klipse",
	"auto-scroll-table",
	"accordion",
	"livereload",
	"anchor-navigation-ex",
	"url-embed",
	"include-csv@^0.1.0",
	"page-treeview",
	"img-layer",
	"embed-pdf",
	"disqus"
    ],
    "pluginsConfig": {
        "insert-logo": {
             "url": "../../Image/icon.jpg",
            "style": "background: none; max-height: 30px; min-height: 30px"
        },
        "favicon": "./icon/book.ico",
	"disqus": {
 	"shortName": "NAME-FROM-DISQUS"
 	 },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy BADT ",
            "modify_label": "该文章修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
	"page-treeview": {
			"copyright": "Copyright &copy BADT",
			"minHeaderCount": "2",
			"minHeaderDeep": "2"
	},
	"anchor-navigation-ex": {
		"showLevel": false,
		"isShowTocTitleIcon": true,
		"tocLevel1Icon": "fa fa-hand-o-right",
		"tocLevel2Icon": "fa fa-hand-o-right",
		"tocLevel3Icon": "fa fa-hand-o-right"
    	}
     }
}